<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Contact Suite">
    <test name="Contacts Group Tests">
        <groups>
            <run>
                <include name="contacts"/>
            </run>
        </groups>
        <classes>
            <class name="api_tests.RestAddNewContactTests"/>
